        {
          intereses.map(({ svg, text }) => {
            return (
              <article class="text-white font-semibold bg-astro-100 p-4 rounded-md grid w-32 justify-items-center hover:scale-110 hover:-rotate-3 hover:shadow-md hover:shadow-orange-500 hover:text-orange-400 duration-200  ">
